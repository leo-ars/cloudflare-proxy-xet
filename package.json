  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/leo-ars/cloudflare-proxy-xet/issues"
  },
  "homepage": "https://github.com/leo-ars/cloudflare-proxy-xet#readme",
  "devDependencies": {
    "@cloudflare/containers": "^0.0.31",
    "wrangler": "^4.58.0"
  }
}
